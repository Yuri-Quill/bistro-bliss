{
	"rewrites": [
		{
			"source": "/assets/:path*",
			"destination": "/assets/:path*"
		},
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=0, must-revalidate"
				}
			]
		}
	],
	"cleanUrls": true
}
